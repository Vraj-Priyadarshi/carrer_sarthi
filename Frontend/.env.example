# ===========================================
# Frontend Environment Variables
# ===========================================
# These values are loaded from the root .env file via symlink
# Or you can copy this to .env and configure manually

# Backend API URL
VITE_API_BASE_URL=http://localhost:8080

# Google OAuth Client ID (same as GOOGLE_CLIENT_ID in root .env)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# App URL
VITE_APP_URL=http://localhost:5173
